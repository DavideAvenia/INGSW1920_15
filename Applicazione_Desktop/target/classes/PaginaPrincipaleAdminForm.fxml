        <Label fx:id="label2" layoutX="14.0" layoutY="23.0" prefHeight="17.0" prefWidth="121.0" text="Cerca per Username :">
            <font>
                <Font name="Gill Sans MT" size="12.0" />
            </font>
        </Label>
        <TextField fx:id="cercausername" layoutX="135.0" layoutY="21.0">
            <font>
                <Font name="Gill Sans MT" size="12.0" />
            </font>
        </TextField>
