<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/11.0.1" fx:controller="Boundary.GestioneUtentiForm">
    <children>
        <ListView fx:id="listaNomiUtenti" layoutX="14.0" layoutY="57.0" prefHeight="326.0" prefWidth="163.0"/>
        <ComboBox fx:id="filtri" layoutX="14.0" layoutY="14.0" prefHeight="30.0" prefWidth="163.0"/>
        <Button fx:id="cancellaUtente" layoutX="190.0" layoutY="356.0" mnemonicParsing="false" text="Cancella"
                onAction="#bottoneCancellaCliccato"/>
        <Button fx:id="aggiornaUtente" layoutX="436.0" layoutY="356.0" mnemonicParsing="false" text="Applica"
                onAction="#bottoneApplicaCliccato"/>
        <Button fx:id="indietro" layoutX="515.0" layoutY="356.0" mnemonicParsing="false" text="Indietro"
                onAction="#bottoneIndietroPremuto"/>
        <TableView fx:id="infoUtente" layoutX="190.0" layoutY="128.0" prefHeight="92.0" prefWidth="399.0">
            <columns>
                <TableColumn prefWidth="75.0" text="C1"/>
                <TableColumn prefWidth="75.0" text="C2"/>
            </columns>
        </TableView>
    </children>
</AnchorPane>
